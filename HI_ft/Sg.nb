Cell[CellGroupData[{
Cell["\<\
Atmospheric Newtonian noise \[LongDash] Homogeneous and isotropic turbulence \
in the frozen approximation\
\>", "Title",
 CellChangeTimes->{{3.862631456254771*^9, 
  3.862631515672855*^9}},ExpressionUUID->"4c54a6b9-b25c-47a6-951f-\
61cc2e87fb99"],

Cell[TextData[{
 "The present notebook contains our computations regarding the \
Newtonian-noise power spectra for homogeneous and isotropic turbulence in the \
frozen approximation. This software is freely available, but if you make use \
of it please cite\n\t[1] D. Brundu, M. Cadoni, P. Olla, M. Oi, and A. P. \
Sanna, \[OpenCurlyDoubleQuote]Atmospheric Newtonian Noise modeling for \
third-generation gravitational wave detectors\[CloseCurlyDoubleQuote], ",
 ButtonBox["Not yet submitted",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL[" "], None},
  ButtonNote->" "],
 "."
}], "Text",
 CellChangeTimes->{{3.8626316228370667`*^9, 3.8626316628949842`*^9}, {
   3.8626329896222897`*^9, 3.8626330902011795`*^9}, {3.8626331301776123`*^9, 
   3.862633150045827*^9}, 3.862633222172676*^9, {3.862633774265807*^9, 
   3.8626337745478354`*^9}},ExpressionUUID->"14974e21-805e-4a3f-9182-\
9abd1de09f0a"],

Cell[CellGroupData[{

Cell["Gravitational-acceleration-perturbation power spectrum", "Section",
 CellChangeTimes->{{3.862631526517025*^9, 
  3.8626315483989134`*^9}},ExpressionUUID->"f03fb34c-3e52-493c-8aa0-\
a5ef844014ac"],

Cell[BoxData[
 RowBox[{
  RowBox[{"$Assumptions", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"r0", ">", "0"}], ",", 
     RowBox[{"kp", ">", "0"}], ",", 
     RowBox[{"k3", "\[Element]", "Reals"}], ",", 
     RowBox[{"xp", ">=", "0"}], ",", 
     RowBox[{"x3", "\[Element]", "Reals"}]}], "}"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.8626332372773275`*^9, 3.862633272636332*^9}, {
  3.862633333936819*^9, 3.8626333512306695`*^9}, {3.862635757209578*^9, 
  3.86263579845224*^9}, {3.8626434460297914`*^9, 3.862643447533716*^9}},
 CellLabel->"In[1]:=",ExpressionUUID->"31abaed3-947d-4789-9a4c-2a8e676ffb58"],

Cell["The gravitational propagator in the real space is", "Text",
 CellChangeTimes->{{3.8626315884472027`*^9, 3.8626315938541794`*^9}, {
  3.862633917501094*^9, 
  3.862633920164503*^9}},ExpressionUUID->"622203eb-60f1-43c9-ab4d-\
db67eed7e994"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"Hx", "=", 
   RowBox[{"\[Alpha]", " ", 
    FractionBox[
     RowBox[{"HeavisideTheta", "[", 
      RowBox[{"x3", "-", "r0"}], "]"}], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["xp", "2"], "+", 
        SuperscriptBox["x3", "2"]}], ")"}], 
      RowBox[{"1", "/", "2"}]]]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Gx", "=", 
   RowBox[{"-", 
    RowBox[{
     SubscriptBox["\[PartialD]", "x1"], 
     RowBox[{"(", 
      RowBox[{"Hx", "/.", 
       RowBox[{
        SuperscriptBox["xp", "2"], "->", 
        RowBox[{
         SuperscriptBox["x1", "2"], "+", 
         SuperscriptBox["x2", "2"]}]}]}], ")"}]}]}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.8626332874985847`*^9, 3.8626334317793417`*^9}, {
  3.862633476756895*^9, 3.862633607658156*^9}, {3.862633860166374*^9, 
  3.8626339066540575`*^9}},
 CellLabel->"In[2]:=",ExpressionUUID->"0b1e555b-c755-4078-9c11-5cab56253be7"],

Cell["\<\
We integrate in cylindrical coordinates because of the symmetries of the \
system\
\>", "Text",
 CellChangeTimes->{{3.862633611763779*^9, 3.8626336150172644`*^9}, {
  3.8626337704973073`*^9, 
  3.862633806277949*^9}},ExpressionUUID->"1c9e460a-3ccd-4fcc-98b1-\
6e985063a7e3"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"I\[Phi]", "=", 
   RowBox[{"Integrate", "[", 
    RowBox[{
     RowBox[{"Hx", " ", 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"\[ImaginaryI]", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"x3", " ", "k3"}], "+", 
          RowBox[{"kp", " ", "xp", " ", 
           RowBox[{"Cos", "[", "\[CapitalPhi]", "]"}]}]}], ")"}]}]]}], ",", 
     RowBox[{"{", 
      RowBox[{"\[CapitalPhi]", ",", "0", ",", 
       RowBox[{"2", "\[Pi]"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Ixp", "=", 
   RowBox[{"Integrate", "[", 
    RowBox[{
     RowBox[{"I\[Phi]", " ", "xp"}], ",", 
     RowBox[{"{", 
      RowBox[{"xp", ",", "0", ",", "\[Infinity]"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"Iz", "=", 
  RowBox[{"Integrate", "[", 
   RowBox[{"Ixp", ",", 
    RowBox[{"{", 
     RowBox[{"x3", ",", "r0", ",", "\[Infinity]"}], "}"}]}], 
   "]"}]}]}], "Input",
 CellChangeTimes->{{3.8626338365459156`*^9, 3.8626338503230696`*^9}, {
  3.862633985363752*^9, 3.862633995462126*^9}, {3.862634115675431*^9, 
  3.8626342210461674`*^9}, {3.8626358459473567`*^9, 3.862635846656679*^9}, {
  3.8626436187070556`*^9, 3.862643626524254*^9}},
 CellLabel->"In[4]:=",ExpressionUUID->"bc0d17ab-e657-4384-aa09-6e2e04ed0a08"],

Cell[BoxData[
 FractionBox[
  RowBox[{"2", " ", 
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{"\[ImaginaryI]", " ", 
     RowBox[{"(", 
      RowBox[{"k3", "+", 
       RowBox[{"\[ImaginaryI]", " ", "kp"}]}], ")"}], " ", "r0"}]], " ", 
   "\[Pi]", " ", "\[Alpha]"}], 
  RowBox[{"kp", " ", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{
      RowBox[{"-", "\[ImaginaryI]"}], " ", "k3"}], "+", "kp"}], 
    ")"}]}]]], "Output",
 CellChangeTimes->{3.862634230002141*^9, 3.8626344894544125`*^9, 
  3.862635824955571*^9, 3.862635865944625*^9, 3.8626434683460493`*^9, 
  3.862643649560066*^9, 3.8641500007669716`*^9},
 CellLabel->"Out[6]=",ExpressionUUID->"50254de3-e27e-45bd-9ead-159a3cedb916"]
}, Open  ]],

Cell[BoxData[
 RowBox[{
  RowBox[{"$Assumptions", "=", 
   RowBox[{"Join", "[", 
    RowBox[{"$Assumptions", ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"0", "<=", "\[Phi]", "<=", 
        RowBox[{"2", "\[Pi]"}]}], ",", 
       RowBox[{"\[Alpha]", ">", "0"}]}], "}"}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.862643588502666*^9, 3.862643642995411*^9}, 
   3.8626438092446375`*^9},
 CellLabel->"In[7]:=",ExpressionUUID->"8e137c23-4fb1-4208-8203-410f11130faf"],

Cell["The gravitational propagator in the Fourier space is", "Text",
 CellChangeTimes->{{3.862643709508639*^9, 
  3.862643723358739*^9}},ExpressionUUID->"78957fb6-687a-4684-b429-\
f4f3cf9bd4b0"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"Gk", "=", 
   RowBox[{"\[ImaginaryI]", " ", "kp", " ", 
    RowBox[{"Cos", "[", "\[Phi]", "]"}], "Iz"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"GkAbs2", "=", 
   RowBox[{
    RowBox[{
     SuperscriptBox[
      RowBox[{"Abs", "[", "Gk", "]"}], "2"], "//", "ComplexExpand"}], "//", 
    "Simplify"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"Print", "[", 
  RowBox[{"\"\<\!\(\*SubscriptBox[\(G\), \(k\)]\) = \>\"", ",", "Gk"}], 
  "]"}]}], "Input",
 CellChangeTimes->{{3.8626434736976576`*^9, 3.8626434895525637`*^9}, {
  3.8626435445162916`*^9, 3.8626435767945347`*^9}, {3.862643657658555*^9, 
  3.862643699558448*^9}},
 CellLabel->"In[8]:=",ExpressionUUID->"bf25e8b0-3529-4d23-b673-3d476f4d3d4a"],

Cell[BoxData[
 InterpretationBox[
  RowBox[{"\<\"\\!\\(\\*SubscriptBox[\\(G\\), \\(k\\)]\\) = \"\>", 
   "\[InvisibleSpace]", 
   FractionBox[
    RowBox[{"2", " ", "\[ImaginaryI]", " ", 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{"\[ImaginaryI]", " ", 
       RowBox[{"(", 
        RowBox[{"k3", "+", 
         RowBox[{"\[ImaginaryI]", " ", "kp"}]}], ")"}], " ", "r0"}]], " ", 
     "\[Pi]", " ", "\[Alpha]", " ", 
     RowBox[{"Cos", "[", "\[Phi]", "]"}]}], 
    RowBox[{
     RowBox[{
      RowBox[{"-", "\[ImaginaryI]"}], " ", "k3"}], "+", "kp"}]]}],
  SequenceForm[
  "\!\(\*SubscriptBox[\(G\), \(k\)]\) = ", Complex[0, 2] 
   E^(Complex[0, 1] ($CellContext`k3 + 
      Complex[0, 1] $CellContext`kp) $CellContext`r0) (
     Complex[0, -1] $CellContext`k3 + $CellContext`kp)^(-1) 
   Pi $CellContext`\[Alpha] Cos[$CellContext`\[Phi]]],
  Editable->False]], "Print",
 CellChangeTimes->{{3.8626436896559334`*^9, 3.86264369994595*^9}, 
   3.8641500009414663`*^9},
 CellLabel->
  "During evaluation of \
In[8]:=",ExpressionUUID->"fb0a9cc4-da73-4050-bf47-b36e82841730"]
}, Open  ]],

Cell["\<\
Now we try to compute the noise spectra via direct integration. We chose to \
integrate this in Cartesian coordinates.\
\>", "Text",
 CellChangeTimes->{{3.8626437357959437`*^9, 3.8626437599257183`*^9}, {
  3.862643813900509*^9, 
  3.8626438497326837`*^9}},ExpressionUUID->"21061026-3864-440f-8604-\
e0524b043001"],

Cell[BoxData[
 RowBox[{
  RowBox[{"$Assumptions", "=", 
   RowBox[{"Join", "[", 
    RowBox[{"$Assumptions", ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"k1", "\[Element]", "Reals"}], ",", 
       RowBox[{"k2", "\[Element]", "Reals"}], ",", 
       RowBox[{"k3", "\[Element]", "Reals"}], ",", 
       RowBox[{"0", "<=", "\[Psi]", "<=", "\[Pi]"}], ",", 
       RowBox[{"\[Omega]", ">", "0"}], ",", 
       RowBox[{"U", ">", "0"}]}], "}"}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.862644113544299*^9, 3.862644162389784*^9}, {
  3.8626465683293066`*^9, 3.8626465748930283`*^9}},
 CellLabel->"In[11]:=",ExpressionUUID->"89f8cf5f-ce61-4725-a5ae-dda8f6c29d51"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"integrand", "=", 
   RowBox[{
    RowBox[{
     FractionBox[
      SubscriptBox["\[ScriptCapitalE]", "T"], 
      RowBox[{
       SuperscriptBox["\[ScriptCapitalE]", 
        RowBox[{"2", "/", "3"}]], 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"2", "\[Pi]"}], ")"}], "3"]}]], 
     SuperscriptBox["k", 
      RowBox[{
       RowBox[{"-", "11"}], "/", "3"}]], "GkAbs2", " ", 
     RowBox[{"(", 
      RowBox[{"2", "\[Pi]", " ", 
       RowBox[{"DiracDelta", "[", 
        RowBox[{"\[Omega]", "-", 
         RowBox[{"kp", " ", "U", " ", 
          RowBox[{"Cos", "[", 
           RowBox[{"\[Phi]", "-", "\[Psi]"}], "]"}]}]}], "]"}]}], ")"}]}], "/.", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"k", "->", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          SuperscriptBox["k1", "2"], "+", 
          SuperscriptBox["k2", "2"], "+", 
          SuperscriptBox["k3", "2"]}], ")"}], 
        RowBox[{"1", "/", "2"}]]}], ",", 
      RowBox[{"kp", "->", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          SuperscriptBox["k1", "2"], "+", 
          SuperscriptBox["k2", "2"]}], ")"}], 
        RowBox[{"1", "/", "2"}]]}]}], "}"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"Print", "[", 
  RowBox[{
  "\"\<\!\(\*SubscriptBox[\(S\), \(g\)]\) = \
\[Integral]\!\(\*SuperscriptBox[\(d\), \(3\)]\)k \>\"", ",", "integrand"}], 
  "]"}]}], "Input",
 CellChangeTimes->{{3.862643855176655*^9, 3.862644073526222*^9}, {
  3.864149943523303*^9, 3.8641499703214307`*^9}},
 CellLabel->"In[12]:=",ExpressionUUID->"17cdeefb-fcf0-48a4-a921-ae2a31eba222"],

Cell[BoxData[
 InterpretationBox[
  RowBox[{"\<\"\\!\\(\\*SubscriptBox[\\(S\\), \\(g\\)]\\) = \[Integral]\\!\\(\
\\*SuperscriptBox[\\(d\\), \\(3\\)]\\)k \"\>", "\[InvisibleSpace]", 
   FractionBox[
    RowBox[{
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       RowBox[{"-", "2"}], " ", 
       SqrtBox[
        RowBox[{
         SuperscriptBox["k1", "2"], "+", 
         SuperscriptBox["k2", "2"]}]], " ", "r0"}]], " ", 
     SuperscriptBox["\[Alpha]", "2"], " ", 
     SuperscriptBox[
      RowBox[{"Cos", "[", "\[Phi]", "]"}], "2"], " ", 
     RowBox[{"DiracDelta", "[", 
      RowBox[{"\[Omega]", "-", 
       RowBox[{
        SqrtBox[
         RowBox[{
          SuperscriptBox["k1", "2"], "+", 
          SuperscriptBox["k2", "2"]}]], " ", "U", " ", 
        RowBox[{"Cos", "[", 
         RowBox[{"\[Phi]", "-", "\[Psi]"}], "]"}]}]}], "]"}], " ", 
     SubscriptBox["\[ScriptCapitalE]", "T"]}], 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["k1", "2"], "+", 
        SuperscriptBox["k2", "2"], "+", 
        SuperscriptBox["k3", "2"]}], ")"}], 
      RowBox[{"17", "/", "6"}]], " ", 
     SuperscriptBox["\[ScriptCapitalE]", 
      RowBox[{"2", "/", "3"}]]}]]}],
  SequenceForm[
  "\!\(\*SubscriptBox[\(S\), \(g\)]\) = \
\[Integral]\!\(\*SuperscriptBox[\(d\), \(3\)]\)k ", 
   E^((-2) ($CellContext`k1^2 + $CellContext`k2^2)^
      Rational[1, 
        2] $CellContext`r0) ($CellContext`k1^2 + $CellContext`k2^2 + \
$CellContext`k3^2)^Rational[-17, 6] $CellContext`\[ScriptCapitalE]^
    Rational[-2, 3] $CellContext`\[Alpha]^2 Cos[$CellContext`\[Phi]]^2 
   DiracDelta[$CellContext`\[Omega] - ($CellContext`k1^2 + $CellContext`k2^2)^
      Rational[1, 2] $CellContext`U 
     Cos[$CellContext`\[Phi] - $CellContext`\[Psi]]] 
   Subscript[$CellContext`\[ScriptCapitalE], $CellContext`T]],
  Editable->False]], "Print",
 CellChangeTimes->{{3.862644052276595*^9, 3.8626440819069076`*^9}, 
   3.862646436315369*^9, 3.864150000973626*^9},
 CellLabel->
  "During evaluation of \
In[12]:=",ExpressionUUID->"8146381e-b735-422b-b347-abd5af5b1da2"]
}, Open  ]],

Cell[TextData[{
 "We rotate the coordinate system in order to align the ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["k", "1"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "8f81eba0-c7a6-4da2-a805-fd0454e2fe3b"],
 "-axis with the detector arm, i.e. ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"cos", "(", 
     RowBox[{"\[Phi]", "-", "\[Psi]"}], ")"}], "->", "cos\[Phi]"}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "f5d2a363-c809-42b0-90df-310c20f30533"],
 ". Because of the symmetries of the integration domain, we see that the term \
",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SuperscriptBox["cos", "2"], "(", 
      RowBox[{"\[Phi]", "+", "\[Psi]"}], ")"}], "->", 
     RowBox[{
      RowBox[{
       SuperscriptBox["cos", "2"], "\[Phi]", " ", 
       SuperscriptBox["cos", "2"], "\[Psi]"}], "+", 
      RowBox[{
       SuperscriptBox["sin", "2"], "\[Phi]", " ", 
       SuperscriptBox["sin", "2"], "\[Psi]"}]}]}], "=", 
    FractionBox[
     RowBox[{
      RowBox[{
       SuperscriptBox[
        SubscriptBox["k", "1"], "2"], 
       SuperscriptBox["cos", "2"], "\[Psi]"}], "+", 
      RowBox[{
       SuperscriptBox[
        SubscriptBox["k", "2"], "2"], 
       SuperscriptBox["sin", "2"], "\[Psi]"}]}], 
     SuperscriptBox[
      SubscriptBox["k", "\[Perpendicular]"], "2"]]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "e5e28211-4eab-4216-874b-9e3ea6cd6c4f"],
 ". "
}], "Text",
 CellChangeTimes->{{3.8626441961398716`*^9, 3.862644259212701*^9}, {
  3.862644384424138*^9, 
  3.862644513148923*^9}},ExpressionUUID->"6629e918-b35d-48b9-9a53-\
99bec185ac44"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"integrandShifted", "=", 
   RowBox[{
    RowBox[{"integrand", "/.", 
     RowBox[{"\[Phi]", "->", 
      RowBox[{"\[Phi]", "+", "\[Psi]"}]}]}], "/.", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"Cos", "[", "\[Phi]", "]"}], "->", 
       FractionBox["k1", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           SuperscriptBox["k1", "2"], "+", 
           SuperscriptBox["k2", "2"]}], ")"}], 
         RowBox[{"1", "/", "2"}]]]}], ",", 
      RowBox[{
       SuperscriptBox[
        RowBox[{"Cos", "[", 
         RowBox[{"\[Phi]", "+", "\[Psi]"}], "]"}], "2"], "->", 
       FractionBox[
        RowBox[{
         RowBox[{
          SuperscriptBox["k1", "2"], 
          SuperscriptBox[
           RowBox[{"Cos", "[", "\[Psi]", "]"}], "2"]}], "+", 
         RowBox[{
          SuperscriptBox["k2", "2"], 
          SuperscriptBox[
           RowBox[{"Sin", "[", "\[Psi]", "]"}], "2"]}]}], 
        RowBox[{
         SuperscriptBox["k1", "2"], "+", 
         SuperscriptBox["k2", "2"]}]]}]}], "}"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"Print", "[", 
  RowBox[{
  "\"\<\!\(\*SubscriptBox[\(S\), \(g\)]\) = \
\[Integral]\!\(\*SuperscriptBox[\(d\), \(3\)]\)k \>\"", ",", 
   "integrandShifted"}], "]"}]}], "Input",
 CellChangeTimes->{{3.8626445285127125`*^9, 3.8626446494770613`*^9}, {
  3.862646432644466*^9, 3.8626464419194717`*^9}},
